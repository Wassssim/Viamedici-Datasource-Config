{
  "name": "webapp-projectsetup",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start-dev": "concurrently \"npm run client-dev\" \"npm run server-dev\"",
    "start-prod": "concurrently \"npm run client-prod\" \"npm run server-prod\"",
    "client-dev": "ng serve --public-host=http://localhost --port 4200 --host 0.0.0.0 --disable-host-check --configuration=dev --hmr --proxy-config proxy.conf.json",
    "client-prod": "ng serve --public-host=http://localhost --port 4200 --host 0.0.0.0 --disable-host-check --configuration=production --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server-dev": "set NODE_ENV=development && ts-node-dev server.ts",
    "server-prod": "set NODE_ENV=production && ts-node server.ts"
  },
  "author": "Roland Stahle",
  "engines": {
    "node": "18.13.0",
    "npm": "^8.19.3"
  },
  "private": true,
